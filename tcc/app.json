{
  "expo": {
    "name": "E-doso",
    "slug": "tcc",
    "version": "1.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/idoso.png",
      "resizeMode": "contain",
      "backgroundColor": "#800080"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [ 
        "CAMERA", 
        "WRITE_EXTERNAL_STORAGE", 
        "CAMERA_ROLL", 
        "WRITE_CONTACTS", 
        "READ_EXTERNAL_STORAGE", 
        "WRITE_EXTERNAL_STORAGE",
        "SYSTEM_ALERT_WINDOW",
        "SET_ALARM" 
      ],
      "package": "com.thebesttcc.tcc",
      "versionCode": 5
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
