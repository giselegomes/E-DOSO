{
  "expo": {
    "name": "tcc",
    "slug": "tcc",
    "version": "1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/idoso.png",
      "resizeMode": "contain",
      "backgroundColor": "#800080"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [ "CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL", "WRITE_CONTACTS" ],
      "package": "com.thebesttcc.tcc",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
